{
  "name": "@hypercubed/dynamo",
  "version": "1.0.2",
  "description": "Fast dynamic method dispatch in typeScript using decorators",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/Hypercubed/dynamo",
  "license": "MIT",
  "keywords": [],
  "files": [
    "build"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s test:lint test:unit",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:lint": "tslint --project .",
    "test:unit": "ava",
    "test:nsp": "nsp check",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "clean": "rimraf build test",
    "bench": "grunion --run \"ts-node <%= file.path %>\" ./benchmarks/*.ts --serial",
    "np": "npm run build && np"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "ava": "^3.0.0",
    "chuhai": "^1.2.0",
    "codecov": "^3.5.0",
    "conditional-type-checks": "^1.0.1",
    "function-overloader": "^2.1.2",
    "grunion": "^1.3.1",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^14.1.1",
    "polymorphic": "^1.4.3",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.3",
    "runtypes": "^4.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.8.0",
    "tslint-config-swimlane": "^3.0.4",
    "tslint-immutable": "^4.5.1",
    "typed-function": "^0.10.7",
    "typescript": "^3.5.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "./tests/*.spec.ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}

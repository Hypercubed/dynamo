{
  "name": "ts-typed-function",
  "version": "0.0.0",
  "description": "Typed-functions in TS using decorators",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/Hypercubed/ts-typed-function",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s test:lint test:unit",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:lint": "tslint --project .",
    "test:unit": "ava",
    "test:nsp": "nsp check",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "rimraf build test",
    "all": "run-s reset test cov:check",
    "prepare-release": "run-s all"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "changelog": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@types/node": "^9.6.1",
    "reflect-metadata": "^0.1.12",
    "typed-function": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^2.1.0",
    "codecov": "^3.5.0",
    "npm-run-all": "^4.1.2",
    "npm-scripts-info": "^0.3.6",
    "nyc": "^14.1.1",
    "opn-cli": "^3.1.0",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.8.0",
    "tslint-config-swimlane": "^3.0.4",
    "tslint-immutable": "^4.5.1",
    "typescript": "^3.5.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "failFast": true,
    "files": [
      "./tests/*.spec.ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "@types/node"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
